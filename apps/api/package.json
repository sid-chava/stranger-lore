{
  "name": "@stranger-lore/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/src/index.js",
    "db:migrate": "prisma migrate dev",
